{
    "name": "bootstrap3",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "bootstrap3 offline",
        "default_popup": "index.html",
        "default_icon": { "19": "/favicon.ico" }
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
        "tabs", 
        "http://*/", 
        "https://*/"
    ]
}