{
    "name": "bootstrap3",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "bootstrap3 offline",
        "default_popup": "index.html",
        "default_icon": { 
          "19": "bootstrap.png",
          "128": "bootstrap.png" 
        }
    },
    "icons": { 
        "16": "bootstrap.png",
        "48": "bootstrap.png",
        "128": "bootstrap.png" },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
        "tabs", 
        "http://*/", 
        "https://*/"
    ]
}